        <mods>
            <li>VV - New Harvest</li>
            <li>VV - New Harvest: Garden Crops</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/BiomeDef[defName="TKKN_Desert"]/wildPlants</xpath>
                    <value>
                        <VV_DragonFruitPlant>0.05</VV_DragonFruitPlant>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/BiomeDef[defName="TKKN_RedwoodForest"]/wildPlants</xpath>
                    <value>
                        <VV_BlackcurrantBush>0.01</VV_BlackcurrantBush>
                        <VV_RosehipBush>0.01</VV_RosehipBush>
                        <VV_BlackberryBush>0.01</VV_BlackberryBush>
                    </value>
                </li>
