<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <!-- When VGP is installed with VPE, VPE renames its peaches to apricots
        creating a duplicate fruit with VV, so lets make ours more specific. -->
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="VV_ApricotTree"]/label</xpath>
        <value>
            <label>goldrich apricot tree</label>
        </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="VV_Apricots"]/label</xpath>
        <value>
            <label>goldrich apricots</label>
        </value>
    </Operation>
</Patch>