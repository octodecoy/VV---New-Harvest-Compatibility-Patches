<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>VV - New Harvest</li>
            <li>VV - New Harvest: Garden Crops</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationConditional">
                    <xpath>/Defs/RecipeDef[@Name="VBE_MixJuiceBase"][not(fixedIngredientFilter/categories/li[text()="VV_NHCP_DummyCategory_Fruit"])]</xpath>
                    <match Class="PatchOperationSequence">
                        <operations>
                            <li Class="PatchOperationAdd">
                                <xpath>/Defs/RecipeDef[defName="VBE_MixJuice"]/ingredients/li/filter/thingDefs</xpath>
                                <value>
                                    <li>VV_Blackberries</li>
                                    <li>VV_Blackcurrants</li>
                                    <li>VV_DragonFruits</li>
                                    <li>VV_Rosehips</li>
                                </value>
                            </li>
                            <li Class="PatchOperationAdd">
                                <xpath>/Defs/RecipeDef[@Name="VBE_MixJuiceBase"]/fixedIngredientFilter/thingDefs</xpath>
                                <value>
                                    <li>VV_Blackberries</li>
                                    <li>VV_Blackcurrants</li>
                                    <li>VV_DragonFruits</li>
                                    <li>VV_Rosehips</li>
                                </value>
                            </li>
                            <li Class="PatchOperationAdd">
                                <xpath>/Defs/RecipeDef[@Name="VBE_MixJuiceBase"]/defaultIngredientFilter/thingDefs</xpath>
                                <value>
                                    <li>VV_Blackberries</li>
                                    <li>VV_Blackcurrants</li>
                                    <li>VV_DragonFruits</li>
                                    <li>VV_Rosehips</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/RecipeDef[defName="VBE_MakeGinMush"]/ingredients/li/filter/thingDefs</xpath>
                    <value>
                        <li>VV_Blackberries</li>
                        <li>VV_Blackcurrants</li>
                        <li>VV_JabuticabaBerries</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/RecipeDef[defName="VBE_MakeGinMush"]/fixedIngredientFilter/thingDefs</xpath>
                    <value>
                        <li>VV_Blackberries</li>
                        <li>VV_Blackcurrants</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/RecipeDef[defName="VBE_MakeGinMush"]/defaultIngredientFilter/thingDefs</xpath>
                    <value>
                        <li>VV_Blackberries</li>
                        <li>VV_Blackcurrants</li>
                    </value>
                </li>
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Vanilla Plants Expanded</li>
                    </mods>
                    <match Class="PatchOperationSequence">
                        <operations>
                            <li Class="PatchOperationAdd">
                                <xpath>/Defs/RecipeDef[defName="VBE_MakeUnfermentedCider"]/ingredients/li/filter/thingDefs</xpath>
                                <value>
                                    <li>VV_DragonFruits</li>
                                </value>
                            </li>
                            <li Class="PatchOperationAdd">
                                <xpath>/Defs/RecipeDef[defName="VBE_MakeUnfermentedCider"]/fixedIngredientFilter/thingDefs</xpath>
                                <value>
                                    <li>VV_DragonFruits</li>
                                </value>
                            </li>
                            <li Class="PatchOperationAdd">
                                <xpath>/Defs/RecipeDef[defName="VBE_MakeUnfermentedCider"]/defaultIngredientFilter/thingDefs</xpath>
                                <value>
                                    <li>VV_DragonFruits</li>
                                </value>
                            </li>
                        </operations>
                    </match>
                </li>
            </operations>
        </match>
    </Operation>
