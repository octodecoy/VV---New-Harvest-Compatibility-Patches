<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <PipeSystem.ProcessDef>
        <defName>VV_AC_VinegarAndSourCherriesToBalsamic</defName>
        <label>Infuse vinegar and sour cherries into balsamic vinegar</label>
        <description>Turns 15 vinegar and 50 sour cherries into 12 balsamic vinegar.</description>
        <useIngredients>true</useIngredients>
        <spawnOnInteractionCell>true</spawnOnInteractionCell>
        <labelOverride>Infuse 15 vinegar and 50 sour cherries into 12 balsamic vinegar</labelOverride>
        <priorityInBillList>10</priorityInBillList>
        <ingredients>
            <li>
                <thing>AC_Vinegar</thing>
                <countNeeded>15</countNeeded>
            </li>
            <li>
                <thing>VV_SourCherrys</thing>
                <countNeeded>50</countNeeded>
            </li>
        </ingredients>
        <ticks>900000</ticks>
        <results>
            <li>
                <thing>AC_BalsamicVinegar</thing>
                <count>12</count>
            </li>
        </results>
    </PipeSystem.ProcessDef>
