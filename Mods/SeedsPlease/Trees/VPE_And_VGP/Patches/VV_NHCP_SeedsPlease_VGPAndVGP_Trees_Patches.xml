<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <!-- If both VPE & VGP are installed then VV apricots are changed to "goldrich apricots", so change seeds to reflect this -->
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="VV_Seed_Apricot"]/label</xpath>
        <value>
            <label>goldrich apricot tree seeds</label>
        </value>
    </Operation>
</Patch>