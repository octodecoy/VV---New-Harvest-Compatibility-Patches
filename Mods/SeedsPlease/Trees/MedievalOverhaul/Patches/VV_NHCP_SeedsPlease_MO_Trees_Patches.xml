<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <!-- If MO installed, change mulberry stuff to reflect change to red -->
    <!-- SeedsPlease doesn't even really work well with Medieval Overhaul, but whatever -->
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="VV_Seed_Mulberry"]/label</xpath>
        <value>
            <label>Red mulberry tree seeds</label>
        </value>
    </Operation>
    <Operation Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName="VV_Seed_Mulberry"]/graphicData/texPath</xpath>
        <value>
            <texPath>SeedsPlease/Seeds/RedMulberry</texPath>
        </value>
    </Operation>
</Patch>