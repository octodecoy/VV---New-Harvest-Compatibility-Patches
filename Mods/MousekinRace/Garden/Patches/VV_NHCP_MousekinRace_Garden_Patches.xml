<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/ThingDef[defName="Mousekin_Windmill"]/comps/li[@Class="PipeSystem.CompProperties_AdvancedResourceProcessor"]/processes</xpath>
        <value>
            <li>VV_NHCP_Mousekin_AmaranthToFlour</li>
            <li>VV_NHCP_Mousekin_MilletToFlour</li>
            <li>VV_NHCP_Mousekin_QuinoaToFlour</li>
            <li>VV_NHCP_Mousekin_TeffToFlour</li>
        </value>
    </Operation>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medieval Overhaul</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/PipeSystem.ProcessDef[defName="VV_NHCP_Mousekin_AmaranthToFlour"]/results/li[thing="Mousekin_Flour"]/thing</xpath>
                    <value>
                        <thing>DankPyon_Flour</thing>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/PipeSystem.ProcessDef[defName="VV_NHCP_Mousekin_MilletToFlour"]/results/li[thing="Mousekin_Flour"]/thing</xpath>
                    <value>
                        <thing>DankPyon_Flour</thing>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/PipeSystem.ProcessDef[defName="VV_NHCP_Mousekin_QuinoaToFlour"]/results/li[thing="Mousekin_Flour"]/thing</xpath>
                    <value>
                        <thing>DankPyon_Flour</thing>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/PipeSystem.ProcessDef[defName="VV_NHCP_Mousekin_TeffToFlour"]/results/li[thing="Mousekin_Flour"]/thing</xpath>
                    <value>
                        <thing>DankPyon_Flour</thing>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/MousekinRace.FlowerListDef[defName="Mousekin_ValidFlowers"]/flowerPlants</xpath>
        <value>
            <li>VV_BlackberryBush</li>
            <li>VV_BlackcurrantBush</li>
        </value>
    </Operation>
</Patch>