<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <PipeSystem.ProcessDef>
        <defName>VV_NHCP_Mousekin_AmaranthToFlour</defName>
        <label>Mill amaranth grain into flour</label>
        <description>Turns 100 amaranth grain into 75 flour</description>
        <spawnOnInteractionCell>true</spawnOnInteractionCell>
        <labelOverride>Mill 100 amaranth grain into 75 flour</labelOverride>
        <priorityInBillList>1</priorityInBillList>
        <ingredients>
            <li>
                <thing>VV_AmaranthGrain</thing>
                <countNeeded>100</countNeeded>
            </li>
        </ingredients>
        <ticks>120000</ticks>
        <results>
            <li>
                <thing>Mousekin_Flour</thing>
                <count>75</count>
            </li>
        </results>
        <autoExtract>false</autoExtract>
    </PipeSystem.ProcessDef>
