<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <PipeSystem.ProcessDef>
        <defName>VV_NHCP_Mousekin_CedarLogToCharcoal</defName>
        <label>Carbonize cedar logs into charcoal</label>
        <description>Turns 300 cedar logs into 600 charcoal.</description>
        <spawnOnInteractionCell>true</spawnOnInteractionCell>
        <labelOverride>Carbonize 300 cedar logs into 600 charcoal</labelOverride>
        <priorityInBillList>1</priorityInBillList>
        <ingredients>
            <li>
                <thing>VV_CedarWood</thing>
                <countNeeded>300</countNeeded>
            </li>
        </ingredients>
        <ticks>180000</ticks>
        <results>
            <li>
                <thing>Mousekin_Charcoal</thing>
                <count>600</count>
            </li>
        </results>
        <autoExtract>false</autoExtract>
    </PipeSystem.ProcessDef>
