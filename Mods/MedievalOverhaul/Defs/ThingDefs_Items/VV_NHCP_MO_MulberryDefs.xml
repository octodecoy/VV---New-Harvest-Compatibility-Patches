<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <!-- White Mulberry to add to VV White Mulberry tree. -->
    <!-- Add regardless of if industrial module is installed, so it can be easily referenced -->
    <ThingDef ParentName="PlantFoodRawBase">
        <defName>VV_WhiteMulberries</defName>
        <label>white mulberries</label>
        <description>Sweet, soft, white berries. Mulberries are great for fresh eating, drying, and making jams.</description>
        <graphicData>
            <texPath>Things/Items/Raw/VV_WhiteMulberries</texPath>
        </graphicData>
        <statBases>
            <MarketValue>1.92</MarketValue>
            <Nutrition>0.08</Nutrition>
        </statBases>
        <ingestible Inherit="False">
            <preferability>RawBad</preferability>
            <ingestEffect>EatVegetarian</ingestEffect>
            <ingestSound>RawVegetable_Eat</ingestSound>
            <foodType>VegetableOrFruit</foodType>
        </ingestible>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>15</daysToRotStart>
                <rotDestroys>true</rotDestroys>
            </li>
        </comps>
    </ThingDef>
</Defs>