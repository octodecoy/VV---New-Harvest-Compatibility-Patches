<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef Name="VV_MO_PlankBase" ParentName="ResourceBase" Abstract="True" MayRequireAnyOf="vvenchov.vvnewharvest,vvenchov.vvnewharvestindustrialcrops">
		<graphicData>
			<texPath>Things/Item/Resource/WoodPlank</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<equippedAngleOffset>-20</equippedAngleOffset>
		<soundInteract>Wood_Drop</soundInteract>
		<soundDrop>Wood_Drop</soundDrop>		
		<burnableByRecipe>true</burnableByRecipe>
		<healthAffectsPrice>false</healthAffectsPrice>
		<minRewardCount>10</minRewardCount>
		<stuffProps>
			<appearance>Planks</appearance>
			<categories>
				<li>Woody</li>
			</categories>
			<constructEffect>ConstructWood</constructEffect>
			<appearance>Planks</appearance>
			<soundImpactBullet>BulletImpact_Wood</soundImpactBullet>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<soundImpactMelee>Pawn_Melee_Punch_HitBuilding_Wood</soundImpactMelee>			
		</stuffProps>
		<isTechHediff>true</isTechHediff>
		<thingCategories>
			<li>DankPyon_Wood</li>
		</thingCategories>
		<techHediffsTags>
			<li>Poor</li>
		</techHediffsTags>
		<terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
		<modExtensions>
			<li Class="MedievalOverhaul.FuelValueProperty">
				<fuelValue>1</fuelValue>
			</li>
		</modExtensions>
	</ThingDef>
	<ThingDef Name="VV_MO_LogBase" ParentName="ResourceBase" Abstract="True" MayRequireAnyOf="vvenchov.vvnewharvest,vvenchov.vvnewharvestindustrialcrops">
        <graphicData>
            <graphicClass>Graphic_StackCount</graphicClass>
        </graphicData>
        <equippedAngleOffset>-20</equippedAngleOffset>
        <soundInteract>Wood_Drop</soundInteract>
        <soundDrop>Wood_Drop</soundDrop>	
        <burnableByRecipe>true</burnableByRecipe>
        <healthAffectsPrice>false</healthAffectsPrice>
        <minRewardCount>10</minRewardCount>	
		<stuffProps>
            <categories>
                <li>DankPyon_RawWood</li>
            </categories>	
			<constructEffect>ConstructWood</constructEffect>
			<appearance>Planks</appearance>
			<soundImpactBullet>BulletImpact_Wood</soundImpactBullet>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<soundImpactMelee>Pawn_Melee_Punch_HitBuilding_Wood</soundImpactMelee>	
			<canSuggestUseDefaultStuff>true</canSuggestUseDefaultStuff>	
		</stuffProps>
        <thingCategories>
            <li>DankPyon_RawWood</li>
        </thingCategories>
        <techHediffsTags>
            <li>Poor</li>
        </techHediffsTags>
        <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
        <allowedArchonexusCount>150</allowedArchonexusCount>
        <modExtensions>
            <li Class="MedievalOverhaul.FuelValueProperty">
                <fuelValue>2</fuelValue>
            </li>
        </modExtensions>
	</ThingDef>
	<!-- LOGS -->
	<!-- Same defNames as the MO C# generated logs, so that saves using the C# generated logs won't be affected. -->
    <ThingDef ParentName="VV_MO_LogBase" MayRequireAnyOf="vvenchov.vvnewharvest,vvenchov.vvnewharvestindustrialcrops">
        <defName>DankPyon_Log_VV_CedarWood</defName>
        <label>cedar wood</label>
        <description>Fragrant, lightweight, unprocessed wood from the cedar tree. Naturally resistant to decay and insects, it is favored for storage chests, roofing, and construction requiring durability without excess weight.</description>
        <graphicData>
            <texPath>Things/Items/Resources/VV_CedarWood</texPath>
        </graphicData>
        <statBases>
			<MarketValue>1.8</MarketValue>
			<MaxHitPoints>200</MaxHitPoints>
			<Mass>0.38</Mass>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>0.1</DeteriorationRate>
			<StuffPower_Armor_Sharp>0.52</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.5</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.4</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>7</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>3</StuffPower_Insulation_Heat>
			<SharpDamageMultiplier>0.5</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.8</BluntDamageMultiplier>
        </statBases>
        <stuffProps>
            <stuffAdjective>cedar</stuffAdjective>
			<commonality>0.01</commonality>
			<color>(201,151,112)</color>
			<statFactors>
				<MaxHitPoints>0.8</MaxHitPoints>
				<Beauty>1.8</Beauty>
				<Flammability>1</Flammability>
				<WorkToMake>1.0</WorkToMake>
				<WorkToBuild>1.0</WorkToBuild>
				<DoorOpenSpeed>1.05</DoorOpenSpeed>
			</statFactors>
        </stuffProps>
        <butcherProducts>
            <VV_CedarWood>2</VV_CedarWood>
        </butcherProducts>
    </ThingDef>
