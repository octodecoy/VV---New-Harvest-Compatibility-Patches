<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>VV - New Harvest</li>
            <li>VV - New Harvest: Garden Crops</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/BiomeDef[defName="ZBiome_GlacialShield"]/wildPlants</xpath>
                    <value>
                        <VV_BlackberryBush>0.01</VV_BlackberryBush>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/BiomeDef[defName="ZBiome_CloudForest"]/wildPlants</xpath>
                    <value>
                        <VV_DragonFruitPlant>0.05</VV_DragonFruitPlant>
                    </value>
