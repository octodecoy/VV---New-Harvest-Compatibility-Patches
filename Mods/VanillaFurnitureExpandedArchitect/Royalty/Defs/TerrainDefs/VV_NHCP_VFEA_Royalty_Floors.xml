<Defs>
    <!-- FINE -->
    <TerrainDef ParentName="VV_NHCP_FloorBase_Cedar">
        <defName>VV_NHCP_VFEArch_FineWoodenFloorCedar</defName>
        <label>cedar vinyl fine wooden floor</label>
        <description>Beautiful wooden parquet, made with no compromises, for expressing economic status. Slow to construct.</description>
        <texturePath>Terrains/FineWoodenFloor</texturePath>
        <statBases>
            <WorkToBuild>5000</WorkToBuild>
            <Beauty>3</Beauty>
            <CleaningTimeFactor>0.8</CleaningTimeFactor>
        </statBases>
        <costList>
            <VV_CedarWood>20</VV_CedarWood>
        </costList>
        <tags>
            <li>FineFloor</li>
        </tags>
        <renderPrecedence>228</renderPrecedence>
        <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    </TerrainDef>
