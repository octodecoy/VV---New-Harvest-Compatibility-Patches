<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <HediffDef>
        <defName>VV_VCE_ConsumedMapleSyrup</defName>
        <label>consumed maple syrup (from meal)</label>
        <makesSickThought>false</makesSickThought>
        <description>Maple syrup is a sweet, amber-colored syrup made by boiling down the sap of sugar maple trees. It has a rich, caramel-like flavor with hints of vanilla and toffee.</description>
        <tendable>false</tendable>
        <isBad>false</isBad>
        <defaultLabelColor>(92,62,198)</defaultLabelColor>
        <hediffClass>HediffWithComps</hediffClass>
        <initialSeverity>1.0</initialSeverity>
        <maxSeverity>1.0</maxSeverity>
        <comps>
            <li Class="VEF.Cooking.HediffCompProperties_WhileHavingThoughts">
                <thoughtDefs>
                    <li>VV_VCE_ConsumedMapleSyrup</li>
                </thoughtDefs>
            </li>
        </comps>
        <stages>
            <li>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.01</offset>
                    </li>
                </capMods>
            </li>
        </stages>
    </HediffDef>
</Defs>