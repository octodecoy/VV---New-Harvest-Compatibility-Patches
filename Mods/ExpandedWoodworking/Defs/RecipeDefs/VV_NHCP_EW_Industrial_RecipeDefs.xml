<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <RecipeDef ParentName="MakeWoodLumberBase">
        <defName>VV_MakeWoodLumberCedar</defName>
        <label>make cedar lumber</label>
        <description>Process raw cedar logs into lumber for construction.</description>
        <allowMixingIngredients>false</allowMixingIngredients>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>VV_CedarWood</li>
                    </thingDefs>
                </filter>
                <count>15</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <thingDefs>
                <li>VV_CedarWood</li>
            </thingDefs>
        </fixedIngredientFilter>
        <products>
            <VV_CedarLumber>30</VV_CedarLumber>
        </products>
    </RecipeDef>
